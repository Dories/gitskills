<?xml version="1.0" encoding="utf-8"?>
<Disclosure xmlns:android="http://schemas.android.com/apk/res/android" >	
	<!--问题卷10 女性健康问卷-->
 <DisclosureItem disclosure_ver="A_10_" disclosure_id="problempieces_10" flag="questionName" text="女性健康问卷"/>

    <DisclosureItem disclosure_ver="A_10_" no_yesno="true" disclosure_id="A_10_1_" flag="common">
        <Description text="1.  您在过去或目前是否出现过以下症状？"/>
            <Radio input_type="checkbox" and_others="false" options="月经（周期、经量）异常" radio_key="A_10_1_a"/>
            <Radio input_type="checkbox" and_others="false" options="白带（量、色）异常" radio_key="A_10_1_b"/>
            <Radio input_type="checkbox" and_others="false" options="阴道异常流血" radio_key="A_10_1_c"/>
            <Radio input_type="checkbox" and_others="false" options="不孕" radio_key="A_10_1_d"/>
            <Radio input_type="checkbox" and_others="false" options="妊娠或产前、产后异常" radio_key="A_10_1_e"/>
            <Radio input_type="checkbox" and_others="false" options="下腹疼痛或不适" radio_key="A_10_1_f"/>
            <Radio input_type="checkbox" and_others="false" options="外阴赘生物" radio_key="A_10_1_g"/>
            <Radio input_type="checkbox" and_others="false" options="停经" radio_key="A_10_1_h"/>
            <Radio input_type="checkbox" and_others="false" options="绝经后再次阴道流血" radio_key="A_10_1_i"/>
            <Radio input_type="checkbox" and_others="false" options="性生活后出血" radio_key="A_10_1_j"/>
            <Radio input_type="checkbox" and_others="false" options="外阴瘙痒" radio_key="A_10_1_k"/>
            <Radio input_type="checkbox" and_others="false" options="其他：" radio_key="A_10_1_l"/>
            <Input input_key="A_10_1_01" length="5" input_type="text" is_checked="false"/>
    </DisclosureItem>
    <DisclosureItem disclosure_ver="A_10_" no_yesno="true" disclosure_id="A_10_2_" flag="common">
        <Description text="2.  若有上述症状，是通过何种方式得知？"/>
            <Radio input_type="checkbox" and_others="false" options="正常体检" radio_key="A_10_2_a"/>
            <Radio input_type="checkbox" and_others="false" options="自觉有症状后检查" radio_key="A_10_2_b"/>
            <Radio input_type="checkbox" and_others="false" options="无意中发现" radio_key="A_10_2_c"/>
    </DisclosureItem>
    <DisclosureItem disclosure_ver="A_10_" no_yesno="true" disclosure_id="A_10_3_" flag="common">
        <Description text="3. 首次出现不适症状的时间："/>
            <Input input_key="A_10_3_year_start" length="3" input_type="number"/>
        <Description text="年"/>
            <Input input_key="A_10_3_month_start" length="3" input_type="number"/>
        <Description text="月   首次得知患妇科疾病的时间： "/>
            <Input input_key="A_10_3_year_know" length="3" input_type="number"/>
        <Description text="年"/>
            <Input input_key="A_10_3_month_know" length="3" input_type="number"/>
        <Description text="月"/>
        <Input input_type="br"/>
        <Description text="医院："/>
            <Input input_key="A_10_3_01" length="5" input_type="text"/>
            <Input input_type="br"/>
        <Description text="诊断："/>
            <Input input_key="A_10_3_02" length="5" input_type="text"/>
    </DisclosureItem>
    <DisclosureItem disclosure_ver="A_10_" no_yesno="true" disclosure_id="A_10_4_" flag="common">
        <Description text="4.  月经初潮年龄："/>
        <Input input_type="br"/>
            <Input input_key="A_10_4_01" length="3" input_type="number"/>
        <Description text="周岁"/>
        <Input input_type="br"/>
        <Description text="周期"/>
            <Input input_key="A_10_4_02" length="3" input_type="number"/>
        <Description text="天"/>
        <Input input_type="br"/>
        <Description text="经期"/>
            <Input input_key="A_10_4_03" length="3" input_type="number"/>
        <Description text="天"/>
        <Input input_type="br"/>
        <Description text="绝经年龄："/>
            <Input input_key="A_10_4_04" length="3" is_checked="false" input_type="number"/>
        <Description text="周岁。"/>
    </DisclosureItem>
    <DisclosureItem disclosure_var="A_10_" no_yesno="false" disclosure_id="A_10_5_" flag="common">
        <Description text="5.  过去是否做过妇科检查？"/>
    </DisclosureItem>
    <DisclosureItem disclosure_var="A_10_" no_yesno="false" disclosure_id="A_10_6_" flag="common">
        <Description text="6.  过去是否做过宫颈脱落细胞学检查（涂片或TCT）、阴道镜、宫腔镜、分泌物培养、盆腔B超等检查？"/>
    </DisclosureItem>
    <DisclosureItem disclosure_var="A_10_" no_yesno="false" disclosure_id="A_10_7_" flag="common">
        <Description text="7.  医生是否建议您治疗？"/>
    </DisclosureItem>
    <DisclosureItem disclosure_var="A_10_" no_yesno="false" disclosure_id="A_10_8_" flag="common">
        <Description text="8.  是否接受过相关治疗？"/>
        <Expendable>
            <Description text="治疗方法："/>
            <Radio input_type="checkbox" and_others="false" options="药物治疗" radio_key="A_10_8_02"/>
            <Radio input_type="checkbox" and_others="false" options="手术治疗" radio_key="A_10_8_03"/>
            <Radio input_type="checkbox" and_others="false" options="住院治疗" radio_key="A_10_8_04"/>
            <Radio input_type="checkbox" and_others="false" options="其他治疗" radio_key="A_10_8_05"/>
            <Input input_key="A_10_8_01" length="5" input_type="text" is_checked="false"/>
        </Expendable>
    </DisclosureItem>
    <DisclosureItem disclosure_var="A_10_" no_yesno="donotCheckYes" disclosure_id="A_10_9_" flag="common">
        <Description text="9.  是否最终确诊？"/>
        <Expendable>
            <Description text="诊断时间"/>
            <Input input_key="A_10_9_01" length="5" input_type="number"/>
            <Description text="医院"/>
            <Input input_key="A_10_9_02" length="5" input_type="text"/>
            <Description text="诊断"/>
            <Input input_key="A_10_9_03" length="5" input_type="text"/>
        </Expendable>
    </DisclosureItem>
    <DisclosureItem disclosure_var="A_10_" no_yesno="false" disclosure_id="A_10_10_" flag="common">
        <Description text="10.  家族成员中有否妇科恶性肿瘤患者（卵巢癌、子宫内膜癌、外阴、阴道癌、宫颈癌等）？"/>
    </DisclosureItem>

    <DisclosureItem disclosure_ver="A_10_" no_yesno="true" disclosure_id="A_10_check_result" flag="BlankRemarkTable" isCheck_yes="false">
        <Description text="以上问题如果选“是”，请详细说明并提供相关的病历资料(门诊病历、住院记录及化验检查结果)"/>
        <Expendable>
             <BlankRemarkTable input_key="A_10_check_result" lines="3"/>
        </Expendable>
    </DisclosureItem>
    <DisclosureItem disclosure_ver="A_10_" no_yesno="false" disclosure_id="A_10_11_" flag="BlankRemarkTable">
        <Description text="11.  是否有以上未提及的其他疾病或不适？"/>
        <Expendable>
             <BlankRemarkTable input_key="A_10_11_01" lines="3"/>
        </Expendable>
    </DisclosureItem>
    
</Disclosure>